{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "types": ["cypress", "@testing-library/cypress"],
        "lib": [
            "es5",
            "dom",
            "es2015.promise"
        ],
        "module": "commonjs",
        "outDir": "build",
        "target": "es5",
        "isolatedModules": false,
        "allowJs": true,
        "noEmit": true
    },
    "baseUrl": "../",
    "include": ["**/*.*"]
}

